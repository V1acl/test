{% extends 'base.html.twig' %}

{% block title %}
    Users
{% endblock %}

{% block body %}
    <table>
        <thead>
            <tr>
                <td>id</td>
                <td>name</td>
                <td>email</td>
            </tr>
        </thead>
        <tbody>
            {% for user in users %}
                <tr>
                    <td>{{ user.id }}</td>
                    <td>{{ user.username }}</td>
                    <td>{{ user.email }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
{% endblock %}